                     &cam_sensor_front0_vana_suspend>;
        gpios = <&tlmm 28 0>,
                <&tlmm 24 0>,
                <&tlmm 101 0>,
                <&tlmm 69 0>;
        gpio-reset = <1>;
        gpio-vana = <2>;
        gpio-vio = <3>;
        gpio-req-tbl-num = <0 1 2 3>;
        gpio-req-tbl-flags = <1 0 0 0>;
        gpio-req-tbl-label = "CAMIF_MCLK3",
